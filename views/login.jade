extends layout
block content
    br
    h1 Login
    h5 Let's Get in from here
    br
    form(method="post",action='/users/login',enctype='multipart/form-data')
        
        .form-group
            label Username
            input.form-control(type="text",name='username',placeholder='Username')
        
        .form-group
            label Password
            input.form-control(type="password",name='password',placeholder='Enter Password')
        input.btn(type="submit",name='submit',value='Login')
        